# :command.initialize
initialize() {
  version="1.0.0"
  long_usage=''
  set -e

}

# :command.run
run() {
  declare -A args=()
  declare -A deps=()
  declare -a other_args=()
  declare -a input=()
  normalize_input "$@"
  parse_requirements "${input[@]}"

  case "$action" in
    "update") liferay_update_command ;;
    "build") liferay_build_command ;;
    "ij") liferay_ij_command ;;
    "run") liferay_run_command ;;
    "morning") liferay_morning_command ;;
    "completions") liferay_completions_command ;;
  esac
}

initialize
run "$@"
